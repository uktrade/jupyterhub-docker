[
  {
    "name": "${container_name}",
    "image": "${container_image}",
    "command": ${container_command},
    "memoryReservation": ${container_memory},
    "cpu": ${container_cpu},
    "essential": true,
    "portMappings": [{
        "containerPort": ${container_port}
    }],
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "${log_group}",
        "awslogs-region": "${log_region}",
        "awslogs-stream-prefix": "${container_name}"
      }
    },
    "environment": [{
      "name": "ADMIN_DB__HOST",
      "value": "${admin_db__host}"
    },
    {
      "name": "ADMIN_DB__NAME",
      "value": "${admin_db__name}"
    },
    {
      "name": "ADMIN_DB__PASSWORD",
      "value": "${admin_db__password}"
    },
    {
      "name": "ADMIN_DB__PORT",
      "value": "${admin_db__port}"
    },
    {
      "name": "ADMIN_DB__USER",
      "value": "${admin_db__user}"
    },
    {
      "name": "ALLOWED_HOSTS__1",
      "value": "${allowed_hosts_1}"
    },
    {
      "name": "ALLOWED_HOSTS__2",
      "value": "${allowed_hosts_2}"
    },
    {
      "name": "AUTHBROKER_CLIENT_ID",
      "value": "${authbroker_client_id}"
    },
    {
      "name": "AUTHBROKER_CLIENT_SECRET",
      "value": "${authbroker_client_secret}"
    },
    {
      "name": "AUTHBROKER_URL",
      "value": "${authbroker_url}"
    },
    {
      "name": "DATA_DB__tiva__HOST",
      "value": "${data_db__tiva__host}"
    },
    {
      "name": "DATA_DB__tiva__NAME",
      "value": "${data_db__tiva__name}"
    },
    {
      "name": "DATA_DB__tiva__PASSWORD",
      "value": "${data_db__tiva__password}"
    },
    {
      "name": "DATA_DB__tiva__PORT",
      "value": "${data_db__tiva__port}"
    },
    {
      "name": "DATA_DB__tiva__USER",
      "value": "${data_db__tiva__user}"
    },
    {
      "name": "DATA_DB__test_1__HOST",
      "value": "${data_db__test_1__host}"
    },
    {
      "name": "DATA_DB__test_1__NAME",
      "value": "${data_db__test_1__name}"
    },
    {
      "name": "DATA_DB__test_1__PASSWORD",
      "value": "${data_db__test_1__password}"
    },
    {
      "name": "DATA_DB__test_1__PORT",
      "value": "${data_db__test_1__port}"
    },
    {
      "name": "DATA_DB__test_1__USER",
      "value": "${data_db__test_1__user}"
    },
    {
      "name": "DATA_DB__test_2__HOST",
      "value": "${data_db__test_2__host}"
    },
    {
      "name": "DATA_DB__test_2__NAME",
      "value": "${data_db__test_2__name}"
    },
    {
      "name": "DATA_DB__test_2__PASSWORD",
      "value": "${data_db__test_2__password}"
    },
    {
      "name": "DATA_DB__test_2__PORT",
      "value": "${data_db__test_2__port}"
    },
    {
      "name": "DATA_DB__test_2__USER",
      "value": "${data_db__test_2__user}"
    },
    {
      "name": "SECRET_KEY",
      "value": "${secret_key}"
    }, {
      "name": "NOTEBOOKS_BUCKET",
      "value": "${notebooks_bucket}"
    }, {
      "name": "NOTEBOOKS_URL",
      "value": "${notebooks_url}"
    }, {
      "name": "APPSTREAM_URL",
      "value": "${appstream_url}"
    }, {
      "name": "SUPPORT_URL",
      "value": "${support_url}"
    }]
  }
]
